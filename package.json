{
  "name": "capstone-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:build": "docker build --platform linux/amd64 -t capstone-demo .",
    "docker:start": "docker run --privileged --rm --platform linux/amd64 -p 3000:3000 -p 3002:3002 -v /var/run/docker.sock:/var/run/docker.sock -v $(pwd):/root --name capstone-demo-dev -it capstone-demo",
    "docker:stop": "docker stop capstone-demo-dev && sleep 1 && docker rmi -f capstone-demo:latest",
    "docker:exec": "docker exec -it capstone-demo-dev bash"
  },
  "dependencies": {
    "@aries-framework/askar": "^0.4.0",
    "@aries-framework/core": "^0.4.2",
    "@aries-framework/node": "^0.4.2",
    "@babel/runtime": "^7.23.5",
    "@hyperledger/aries-askar-nodejs": "^0.1.0",
    "@lehigh-oracle-did23/aries-framework-oracle": "^1.0.2",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-login-page": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "overrides": {
    "ref-napi": "npm:@2060.io/ref-napi"
  }
}
